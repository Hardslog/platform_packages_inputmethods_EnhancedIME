<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <include latin:keyboardLayout="@xml/key_styles_common" />
    <include latin:keyboardLayout="@xml/key_styles_currency" />
    <Row latin:keyWidth="9.000004%p">
        <include latin:keyboardLayout="@xml/rowkeys_symbols0" />
        <Key latin:keyStyle="tabKeyStyle" latin:keyWidth="fillRight" />
    </Row>
    <Row latin:keyWidth="9.000004%p">
        <include latin:keyboardLayout="@xml/rowkeys_symbols_shift1" />
        <Key latin:keyStyle="deleteKeyStyle" latin:keyWidth="fillRight" />
    </Row>
    <Row latin:keyWidth="9.000004%p">
        <include latin:keyXPos="4.4999957%p" latin:keyboardLayout="@xml/rowkeys_symbols_shift2" />
        <Key latin:keyStyle="enterKeyStyle" latin:keyWidth="fillRight" />
    </Row>
    <Row latin:keyWidth="9.000004%p">
        <Key latin:keyStyle="backFromMoreSymbolKeyStyle" latin:keyWidth="10.000002%p" />
        <include latin:keyboardLayout="@xml/rowkeys_symbols_shift3" />
        <Key latin:keySpec="¡" />
        <Key latin:keySpec="¿" />
        <Key latin:keyStyle="backFromMoreSymbolKeyStyle" latin:keyWidth="fillRight" />
    </Row>
    <Row latin:backgroundType="functional" latin:keyWidth="9.000004%p">
        <Key latin:keyStyle="toAlphaKeyStyle" latin:keyWidth="10.000002%p" />
        <include latin:keyboardLayout="@xml/row_symbols_shift4" />
        <include latin:keyboardLayout="@xml/key_emoji" />
    </Row>
</merge>