<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <include latin:keyboardLayout="@xml/key_styles_common" />
    <include latin:keyboardLayout="@xml/key_styles_currency" />
    <Row latin:keyWidth="10.000002%p">
        <include latin:keyboardLayout="@xml/rowkeys_symbols0" />
    </Row>
    <Row latin:keyWidth="10.000002%p">
        <include latin:keyboardLayout="@xml/rowkeys_symbols_shift1" />
    </Row>
    <Row latin:keyWidth="10.000002%p">
        <include latin:keyXPos="4.999995%p" latin:keyboardLayout="@xml/rowkeys_symbols_shift2" />
    </Row>
    <Row latin:keyWidth="10.000002%p">
        <Key latin:keyStyle="backFromMoreSymbolKeyStyle" latin:visualInsetsRight="0.99999905%p" latin:keyWidth="14.999998%p" />
        <include latin:keyboardLayout="@xml/rowkeys_symbols_shift3" />
        <Key latin:keyStyle="deleteKeyStyle" latin:visualInsetsLeft="0.99999905%p" latin:keyWidth="fillRight" />
    </Row>
    <Row latin:backgroundType="functional" latin:keyWidth="10.000002%p">
        <Key latin:keyStyle="toAlphaKeyStyle" latin:keyWidth="14.999998%p" />
        <include latin:keyboardLayout="@xml/row_symbols_shift4" />
        <Key latin:keyStyle="enterKeyStyle" latin:keyWidth="fillRight" />
    </Row>
</merge>