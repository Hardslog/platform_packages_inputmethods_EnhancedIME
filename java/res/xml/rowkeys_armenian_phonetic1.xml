<merge xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <!-- U+0567: "է" ARMENIAN SMALL LETTER EH -->
    <Key
        latin:keySpec="&#x0567;"
        latin:keyHintLabel="!text/keyspec_left_curly_bracket"
        latin:additionalMoreKeys="!text/keyspec_left_curly_bracket" />
    <!-- U+0569: "թ" ARMENIAN SMALL LETTER TO -->
    <Key
        latin:keySpec="&#x0569;"
        latin:keyHintLabel="!text/keyspec_right_curly_bracket"
        latin:additionalMoreKeys="!text/keyspec_right_curly_bracket" />
    <!-- U+0583: "փ" ARMENIAN SMALL LETTER PIWR -->
    <Key
        latin:keySpec="&#x0583;"
        latin:keyHintLabel="~"
        latin:additionalMoreKeys="~" />
    <!-- U+0571: "ձ" ARMENIAN SMALL LETTER JA -->
    <Key
        latin:keySpec="&#x0571;"
        latin:keyHintLabel="|"
        latin:additionalMoreKeys="|" />
    <!-- U+057B: "ջ" ARMENIAN SMALL LETTER JHEH -->
    <Key
        latin:keySpec="&#x057B;"
        latin:keyHintLabel="\\"
        latin:additionalMoreKeys="\\" />
    <!-- U+0580: "ր" ARMENIAN SMALL LETTER REH -->
    <Key
        latin:keySpec="&#x0580;"
        latin:keyHintLabel="="
        latin:additionalMoreKeys="=" />
    <!-- U+0579: "չ" ARMENIAN SMALL LETTER CHA -->
    <Key
        latin:keySpec="&#x0579;"
        latin:keyHintLabel="÷"
        latin:additionalMoreKeys="÷" />
    <!-- U+0573: "ճ" ARMENIAN SMALL LETTER CHEH -->
    <Key
        latin:keySpec="&#x0573;"
        latin:keyHintLabel="×"
        latin:additionalMoreKeys="×" />
    <!-- U+056A: "ժ" ARMENIAN SMALL LETTER ZHE -->
    <Key
        latin:keySpec="&#x056A;"
        latin:keyHintLabel="["
        latin:additionalMoreKeys="[" />
    <!-- U+056E: "ծ" ARMENIAN SMALL LETTER CA -->
    <Key
        latin:keySpec="&#x056E;"
        latin:keyHintLabel="]"
        latin:additionalMoreKeys="]" />
</merge>
