<merge
    xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin"
>
    <!-- U+0459: "љ" CYRILLIC SMALL LETTER LJE -->
    <Key
        latin:keySpec="&#x0459;"
        latin:keyHintLabel="!text/keyspec_left_curly_bracket"
        latin:additionalMoreKeys="!text/keyspec_left_curly_bracket" />
    <!-- U+045A: "њ" CYRILLIC SMALL LETTER NJE -->
    <Key
        latin:keySpec="&#x045A;"
        latin:keyHintLabel="!text/keyspec_right_curly_bracket"
        latin:additionalMoreKeys="!text/keyspec_right_curly_bracket" />
    <!-- U+0435: "е" CYRILLIC SMALL LETTER IE -->
    <Key
        latin:keySpec="&#x0435;"
        latin:keyHintLabel="~"
        latin:additionalMoreKeys="~"
        latin:moreKeys="!text/morekeys_cyrillic_ie" />
    <!-- U+0440: "р" CYRILLIC SMALL LETTER ER -->
    <Key
        latin:keySpec="&#x0440;"
        latin:keyHintLabel="|"
        latin:additionalMoreKeys="|" />
    <!-- U+0442: "т" CYRILLIC SMALL LETTER TE -->
    <Key
        latin:keySpec="&#x0442;"
        latin:keyHintLabel="\\"
        latin:additionalMoreKeys="\\" />
    <Key
        latin:keySpec="!text/keyspec_south_slavic_row1_6"
        latin:keyHintLabel="="
        latin:additionalMoreKeys="=" />
    <!-- U+0443: "у" CYRILLIC SMALL LETTER U -->
    <Key
        latin:keySpec="&#x0443;"
        latin:keyHintLabel="÷"
        latin:additionalMoreKeys="÷" />
    <!-- U+0438: "и" CYRILLIC SMALL LETTER I -->
    <Key
        latin:keySpec="&#x0438;"
        latin:keyHintLabel="×"
        latin:additionalMoreKeys="×"
        latin:moreKeys="!text/morekeys_cyrillic_i" />
    <!-- U+043E: "о" CYRILLIC SMALL LETTER O -->
    <Key
        latin:keySpec="&#x043E;"
        latin:keyHintLabel="["
        latin:additionalMoreKeys="[" />
    <!-- U+043F: "п" CYRILLIC SMALL LETTER PE -->
    <Key
        latin:keySpec="&#x043F;"
        latin:keyHintLabel="]"
        latin:additionalMoreKeys="]" />
    <!-- U+0448: "ш" CYRILLIC SMALL LETTER SHA -->
    <Key
        latin:keySpec="&#x0448;" />
</merge>
