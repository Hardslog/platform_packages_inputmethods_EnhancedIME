<merge
    xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin"
>
    <switch>
        <case
            latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted"
        >
            <Key
                latin:keySpec="Q"
                latin:keyHintLabel="!text/keyspec_left_curly_bracket"
                latin:additionalMoreKeys="!text/keyspec_left_curly_bracket" />
            <!-- U+10ED: "ჭ" GEORGIAN LETTER CHAR -->
            <Key
                latin:keySpec="&#x10ED;"
                latin:keyHintLabel="!text/keyspec_right_curly_bracket"
                latin:additionalMoreKeys="!text/keyspec_right_curly_bracket" />
            <Key
                latin:keySpec="E"
                latin:keyHintLabel="~"
                latin:additionalMoreKeys="~" />
            <!-- U+10E6: "ღ" GEORGIAN LETTER GHAN -->
            <Key
                latin:keySpec="&#x10E6;"
                latin:keyHintLabel="|"
                latin:additionalMoreKeys="|" />
            <!-- U+10D7: "თ" GEORGIAN LETTER TAN -->
            <Key
                latin:keySpec="&#x10D7;"
                latin:keyHintLabel="\\"
                latin:additionalMoreKeys="\\" />
            <Key
                latin:keySpec="Y"
                latin:keyHintLabel="="
                latin:additionalMoreKeys="=" />
            <Key
                latin:keySpec="U"
                latin:keyHintLabel="÷"
                latin:additionalMoreKeys="÷" />
            <Key
                latin:keySpec="I"
                latin:keyHintLabel="×"
                latin:additionalMoreKeys="×" />
            <Key
                latin:keySpec="O"
                latin:keyHintLabel="["
                latin:additionalMoreKeys="[" />
            <Key
                latin:keySpec="P"
                latin:keyHintLabel="]"
                latin:additionalMoreKeys="]" />
        </case>
        <default>
            <!-- U+10E5: "ქ" GEORGIAN LETTER GHAN -->
            <Key
                latin:keySpec="&#x10E5;"
                latin:keyHintLabel="!text/keyspec_left_curly_bracket"
                latin:additionalMoreKeys="!text/keyspec_left_curly_bracket" />
            <!-- U+10EC: "წ" GEORGIAN LETTER CIL -->
            <Key
                latin:keySpec="&#x10EC;"
                latin:keyHintLabel="!text/keyspec_right_curly_bracket"
                latin:additionalMoreKeys="!text/keyspec_right_curly_bracket" />
            <!-- U+10D4: "ე" GEORGIAN LETTER EN
                 U+10F1: "ჱ" GEORGIAN LETTER HE -->
            <Key
                latin:keySpec="&#x10D4;"
                latin:moreKeys="&#x10F1;"
                latin:keyHintLabel="~"
                latin:additionalMoreKeys="~" />
            <!-- U+10E0: "რ" GEORGIAN LETTER RAE -->
            <Key
                latin:keySpec="&#x10E0;"
                latin:keyHintLabel="|"
                latin:additionalMoreKeys="|" />
            <!-- U+10E2: "ტ" GEORGIAN LETTER TAR -->
            <Key
                latin:keySpec="&#x10E2;"
                latin:keyHintLabel="\\"
                latin:additionalMoreKeys="\\" />
            <!-- U+10E7: "ყ" GEORGIAN LETTER QAR
                 U+10F8: "ჸ" GEORGIAN LETTER ELIFI -->
            <Key
                latin:keySpec="&#x10E7;"
                latin:moreKeys="&#x10F8;"
                latin:keyHintLabel="="
                latin:additionalMoreKeys="=" />
            <!-- U+10E3: "უ" GEORGIAN LETTER UN -->
            <Key
                latin:keySpec="&#x10E3;"
                latin:keyHintLabel="÷"
                latin:additionalMoreKeys="÷" />
            <!-- U+10D8: "ი" GEORGIAN LETTER IN
                 U+10F2: "ჲ" GEORGIAN LETTER HIE -->
            <Key
                latin:keySpec="&#x10D8;"
                latin:moreKeys="&#x10F2;"
                latin:keyHintLabel="×"
                latin:additionalMoreKeys="×" />
            <!-- U+10DD: "ო" GEORGIAN LETTER ON -->
            <Key
                latin:keySpec="&#x10DD;"
                latin:keyHintLabel="["
                latin:additionalMoreKeys="[" />
            <!-- U+10DE: "პ" GEORGIAN LETTER PAR -->
            <Key
                latin:keySpec="&#x10DE;"
                latin:keyHintLabel="]"
                latin:additionalMoreKeys="]" />
        </default>
    </switch>
</merge>
